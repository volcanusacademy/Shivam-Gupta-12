// Program to find out whether entered number is even or odd

#include <stdio.h>
int main() {
    int num;
    printf("Enter an Number: ");
    scanf("%d", &num);

    
    if(num % 2 == 0)
        printf("%d Number is Even.", num);
    else
        printf("%d Number is Odd.", num);
    
    return 0;
}