//Program to find out greater number among three numbers and test for all possible equalities


#include <stdio.h>

int main() {

  int n1, n2, n3;

  printf("Enter first numbers: ");
  scanf("%d", &n1);
  
    printf("Enter second numbers: ");
  scanf("%d", &n2);

  printf("Enter third numbers: ");
  scanf("%d", &n3);


  if (n1 >= n2 && n1 >= n3)
    printf("First number is the largest number.%d", n1);

  if (n2 >= n1 && n2 >= n3)
    printf("Second number is the largest number.%d", n2);

  if (n3 >= n1 && n3 >= n2)
    printf("Third number is the largest number.%d", n3);
    
     

  return 0;
}